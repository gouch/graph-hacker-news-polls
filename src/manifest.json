{
    "name": "Graph Hacker News Polls",
    "description": "Adds a bar graph to the results of Hacker News polls.",
    "content_scripts": [{
        "matches": ["http://news.ycombinator.com/item*"],
        "js": ["script.js"]
    }],
    "icons": {
        "48": "icon-48.png", 
        "128": "icon-128.png"
    },
    "update_url": "https://github.com/gouch/graph-hacker-news-polls/raw/master/updates.xml",
    "version": "1.3"
}
